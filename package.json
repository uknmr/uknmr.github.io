{
  "name": "uknmr.github.io",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf docs/",
    "mkdir:css": "mkdirp docs/css",
    "build": "npm-run-all clean --parallel build:*",
    "build:css": "run-s mkdir:css tailwindcss",
    "build:11ty": "eleventy",
    "watch": "eleventy --watch",
    "serve": "run-s clean build:css 'build:11ty as-is --serve'",
    "tailwindcss": "node src/build-css.js style"
  },
  "repository": "ssh://git@github.com/uknmr/uknmr.github.io.git",
  "author": "uknmr <kanamori@mediba.jp>",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "clean-css": "^4.2.1",
    "luxon": "^1.17.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "pretty-hrtime": "^1.0.3",
    "rimraf": "^3.0.0",
    "tailwindcss": "^1.1.2"
  }
}
