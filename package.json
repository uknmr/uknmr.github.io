{
  "name": "uknmr.github.io",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf docs/",
    "mkdir:css": "mkdirp docs/css docs/scripts",
    "build": "npm-run-all clean --parallel build:* copy:*",
    "build:css": "run-s mkdir:css tailwindcss",
    "build:11ty": "eleventy",
    "copy:js": "cpx src/scripts/*.js docs/scripts",
    "watch": "eleventy --watch",
    "serve": "run-s clean build:css copy:* 'build:11ty as-is --serve'",
    "tailwindcss": "node src/build-css.js style"
  },
  "repository": "ssh://git@github.com/uknmr/uknmr.github.io.git",
  "author": "uknmr <kanamori@mediba.jp>",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "clean-css": "^4.2.1",
    "cpx": "^1.5.0",
    "luxon": "^1.21.3",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "pretty-hrtime": "^1.0.3",
    "rimraf": "^3.0.0",
    "tailwindcss": "^1.1.3"
  }
}
